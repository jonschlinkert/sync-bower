    "lazy-cache": "^2.0.2",
    "load-pkg": "^3.0.1",
    "log-utils": "^0.2.1",
    "minimist": "^1.2.0",
    "normalize-pkg": "^0.4.1",
    "object.omit": "^2.0.1",
